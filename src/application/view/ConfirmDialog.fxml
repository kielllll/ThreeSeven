<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="250.0" prefWidth="400.0" style="-fx-background-radius: 30; -fx-border-radius: 30; -fx-border-color: #e4e2e9;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <AnchorPane prefHeight="70.0" prefWidth="400.0" style="-fx-background-color: #c96332; -fx-background-radius: 30 30 0 0; -fx-border-radius: 30 30 0 0;" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblHeader" layoutX="14.0" layoutY="20.0" text="Warning Message" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane prefHeight="188.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblContent" layoutX="14.0" layoutY="22.0" text="Content" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="46.0" prefWidth="400.0" style="-fx-background-color: #c96332; -fx-background-radius: 0 0 30 30; -fx-border-radius: 0 0 30 30;" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="btnOK" layoutX="123.0" layoutY="11.0" text="OK" textFill="WHITE" />
            <JFXButton fx:id="btnCancel" layoutX="215.0" layoutY="11.0" text="Cancel" textFill="WHITE" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
